{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-modules-commonjs"
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "utils": "./src/utils",
                    "components": "./src/components"
                }
            }
        ]
    ]
}